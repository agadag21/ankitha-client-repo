{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    }
  ]
}
